{
  "name": "ungite",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@9.1.3",
  "description": "Easily to control git.",
  "author": "Chris <https://github.com/zyyv>",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/ungite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/ungite.git"
  },
  "bugs": {
    "url": "https://github.com/zyyv/ungite/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "ungite": "./bin/ungite.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "lint": "eslint .",
    "test": "vitest",
    "test:update": "vitest -u",
    "typecheck": "tcs --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@babel/types": "^7.24.0",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^9.1.1",
    "esno": "^4.7.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  }
}
